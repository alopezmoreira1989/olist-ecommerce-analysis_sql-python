   "source": [
    "q14 = \"\"\"\n",
    "SELECT COUNT(*) AS frequent_customers\n",
    "FROM (\n",
    "    SELECT c.customer_unique_id\n",
    "    FROM customers c\n",
    "    JOIN orders o ON c.customer_id = o.customer_id\n",
    "    GROUP BY c.customer_unique_id\n",
    "    HAVING COUNT(o.order_id) > 1\n",
    ") AS frequent_customers_subquery;\n",
    "\n",
    "\"\"\""
   ]
  },
