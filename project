   "source": [
    "q23 = '''\n",
    "SELECT \n",
    "    c.customer_state,\n",
    "    SUM(op.payment_value) AS total_sales\n",
    "FROM customers c\n",
    "JOIN orders o ON c.customer_id = o.customer_id\n",
    "JOIN order_payments op ON o.order_id = op.order_id\n",
    "GROUP BY c.customer_state\n",
    "ORDER BY total_sales DESC\n",
    "LIMIT 10;\n",
    "'''"
   ]
  },
