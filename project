    "## Q2.1: \"The 10 Most Demanded Product Categories\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "2bf7d0fa",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-10-12T11:56:47.753211Z",
     "iopub.status.busy": "2025-10-12T11:56:47.752853Z",
     "iopub.status.idle": "2025-10-12T11:56:47.757850Z",
     "shell.execute_reply": "2025-10-12T11:56:47.756905Z"
    },
    "papermill": {
     "duration": 0.020348,
     "end_time": "2025-10-12T11:56:47.759740",
     "exception": false,
     "start_time": "2025-10-12T11:56:47.739392",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "q21 = \"\"\"\n",
    "SELECT \n",
    "    t.product_category_name_english AS product_category_name,\n",
    "    COUNT(oi.order_id) AS total_orders_per_category\n",
    "FROM \n",
    "    order_items AS oi\n",
    "JOIN \n",
    "    products AS p\n",
    "    ON oi.product_id = p.product_id\n",
    "JOIN\n",
    "    product_category_name_translation AS t\n",
    "    ON p.product_category_name = t.product_category_name\n",
    "GROUP BY \n",
    "    t.product_category_name_english\n",
    "ORDER BY \n",
    "    total_orders_per_category DESC\n",
    "LIMIT 10;\n",
    "\"\"\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "id": "2f37378e",
   "metadata": {
    "execution": {
